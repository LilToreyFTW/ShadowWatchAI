{
  "shadowwatch": {
    "version": "1.0.0",
    "name": "ShadowWatch AI - Ultimate Game Development Assistant",
    "description": "Portable AI software for game development",
    "features": [
      "autonomous-development",
      "ultra-maximum-9500h-mode",
      "unity-csharp-integration",
      "unreal-cpp-integration",
      "web-typescript-javascript",
      "weapon-blueprint-creation",
      "vehicle-blueprint-creation",
      "3d-model-generation",
      "model-pack-management",
      "organized-directory-structure",
      "anti-hacker-protection",
      "continuous-security-monitoring",
      "language-restrictions-enforced",
      "enterprise-security-features"
    ],
    "supportedLanguages": {
      "unity": ["csharp"],
      "unreal": ["cpp"],
      "web": ["typescript", "javascript"],
      "restricted": ["python", "java", "blueprints"]
    },
    "developmentModes": {
      "autonomous": {
        "description": "Basic AI-assisted development",
        "duration": "manual",
        "parallelTasks": 3
      },
      "ultraMaximum": {
        "description": "9500 hours of continuous development",
        "duration": "9500h",
        "parallelTasks": 10
      }
    }
  },
  "server": {
    "port": 8080,
    "host": "localhost",
    "cors": true,
    "compression": true,
    "helmet": true
  },
  "ai": {
    "model": "AUTO",
    "maxParallelTasks": 10,
    "developmentInterval": 15000,
    "autoSaveInterval": 60000,
    "securityScanInterval": 180000
  },
  "engines": {
    "unity": {
      "version": "2022.3+",
      "language": "csharp-only",
      "scriptingBackend": "mono",
      "apiCompatibility": "netstandard2.1"
    },
    "unreal": {
      "version": "5.3+",
      "language": "cpp-only",
      "blueprintsAllowed": false,
      "visualStudio": "required"
    },
    "web": {
      "languages": ["typescript", "javascript"],
      "frameworks": ["vanilla", "three.js", "babylon.js"],
      "browsers": ["chrome", "firefox", "safari", "edge"]
    }
  },
  "directories": {
    "models": "models/",
    "weapons": "models/weapons/",
    "vehicles": "models/vehicles/",
    "characters": "models/characters/",
    "environments": "models/environments/",
    "props": "models/props/",
    "effects": "models/effects/",
    "audio": "models/audio/",
    "textures": "models/textures/",
    "blueprints": "models/blueprints/",
    "scripts": "models/scripts/"
  },
  "security": {
    "antiHackerProtection": true,
    "codeIntegrityChecks": true,
    "redistributionPrevention": true,
    "apiKeyProtection": true,
    "logSecurityEvents": true,
    "autoSecurityUpdates": true
  },
  "logging": {
    "enabled": true,
    "level": "info",
    "files": {
      "server": "logs/server.log",
      "ai": "logs/ai-activity.log",
      "security": "logs/security.log",
      "development": "logs/development.log"
    },
    "maxSize": "10m",
    "maxFiles": 5
  }
}
